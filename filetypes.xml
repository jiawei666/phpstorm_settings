<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.o;*.obj;*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc;" />
    <extensionMap>
      <mapping pattern="test" type="AUTO_DETECTED" />
      <mapping pattern="Dockerfile-logstash" type="Dockerfile" />
      <mapping ext="bak" type="PLAIN_TEXT" />
      <mapping ext="log" type="PLAIN_TEXT" />
      <mapping ext="backup" type="PLAIN_TEXT" />
      <removed_mapping ext="bash" approved="true" type="Shell Script" />
      <removed_mapping ext="sh" approved="true" type="Shell Script" />
      <removed_mapping ext="zsh" approved="true" type="Shell Script" />
    </extensionMap>
  </component>
</application>